{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "grammar",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "terminal_definition"
        },
        {
          "type": "SYMBOL",
          "name": "rule_definition"
        }
      ]
    },
    "terminal_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "terminal"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "rule_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "rule"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "STRING",
          "value": "->"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "SYMBOL",
            "name": "choice"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "choice": {
      "type": "SYMBOL",
      "name": "_choice"
    },
    "_choice": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sequence"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "sequence"
            },
            {
              "type": "STRING",
              "value": "|"
            },
            {
              "type": "SYMBOL",
              "name": "_choice"
            }
          ]
        }
      ]
    },
    "sequence": {
      "type": "SYMBOL",
      "name": "_sequence"
    },
    "_sequence": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_rule_element"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_rule_element"
            },
            {
              "type": "SYMBOL",
              "name": "_sequence"
            }
          ]
        }
      ]
    },
    "_rule_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_thing"
        },
        {
          "type": "SYMBOL",
          "name": "affectation"
        }
      ]
    },
    "_thing": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "_grouping"
        }
      ]
    },
    "_grouping": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "choice"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "affectation": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_thing"
          }
        }
      ]
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "(\\\\+(.|\\r?\\n)|[^\\\\\\n])*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s|\\\\\\r?\\n"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
